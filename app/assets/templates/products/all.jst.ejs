<div class="shop-management"></div>
<div class="row">
  <div class="product-item-wrapper col-md-6 col-md-offset-3">
    <% products.each( function(product) { %>
      <div class="product-item"><a href="#/products/<%= product.get("id")%>">
        <img src="<%= product.get("product_image")%>" width="auto" height="200">
        <div>
          <%= product.get("name") %>
          $<%= product.get("price") %>
        </div>
        </a>
      </div>
    <% }) %>
  </div>
</div>
