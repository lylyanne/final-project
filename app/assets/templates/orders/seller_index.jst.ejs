<h3 class="text-center">All my sales</h3>

<div class="row">
  <div class="col-xs-6 col-xs-offset-3">
    <% if (orders.length == 0) { %>
    <p class="text-center">
      You don't have any sales yet.
    </p>
    <% } else { %>
    <% orders.each(function (order) { %>
      <div class="well">

        <div class="row">
          <div class="order"></div>

        </div>
      </div>
      <% }) %>
    <% } %>
  </div>
</div>
